{
  "name": "boilerplate",
  "version": "0.0.1",
  "scripts": {
    "serve": "ng serve --configuration development --host 0.0.0.0  --port 8000",
    "build:test": "ng build --configuration test && cp .htaccess dist/test/browser/.htacces",
    "build:prod": "ng build --base-href ./ --configuration production && cp .htaccess dist/production/browser/.htacces",
    "build:deploy": "node cli/build-versioned.js",
    "build:all": "npm run build:test && npm run build:prod",
    "make:domain": "node cli/make-domain.js make:domain",
    "make:domain:file": "node cli/make-domain-file.js make:domain:file",
    "md": "npm run make:domain",
    "mdf": "npm run make:domain:file"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.9",
    "@angular/common": "^20.3.6",
    "@angular/compiler": "^20.3.6",
    "@angular/core": "^20.3.6",
    "@angular/forms": "^20.3.6",
    "@angular/material": "^20.2.9",
    "@angular/platform-browser": "^20.3.6",
    "@angular/platform-browser-dynamic": "^20.3.6",
    "@angular/router": "^20.3.6",
    "animate.css": "^4.1.1",
    "ansi-to-html": "^0.7.2",
    "highlight.js": "^11.11.1",
    "highlight.run": "^9.22.3",
    "ngx-highlightjs": "^14.0.1",
    "ngx-mask": "^20.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.6",
    "@angular/cli": "^20.3.6",
    "@angular/compiler-cli": "^20.3.6",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.21",
    "commander": "^13.1.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3"
  }
}
